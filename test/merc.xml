<?xml version="1.0" encoding="utf-8"?>
<Map srs="+init=epsg:3857" background-color="steelblue">
    <Style name="styleA">
        <Rule>
            <PolygonSymbolizer fill="rgba(255, 0, 0, 0.5)" />
        </Rule>
    </Style>

    <Layer name="layerA" srs="+init=epsg:4326">
        <StyleName>styleA</StyleName>
        <Datasource>
            <Parameter name="file">merc.geojson</Parameter>
            <Parameter name="type">geojson</Parameter>
        </Datasource>
    </Layer>

</Map>
